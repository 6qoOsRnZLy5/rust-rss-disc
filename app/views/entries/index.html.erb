<p id="notice"><%= notice %></p>

<h1>Entries</h1>

<table>
  <thead>
    <tr>
      <th>Feed</th>
      <th>Title</th>
      <th>Description</th>
      <th>Picture</th>
      <th>Link1</th>
      <th>Link2</th>
      <th>Guid</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entries.each do |entry| %>
      <tr>
        <td><%= link_to @feed.title, feed_entry_path(@feed, entry) %></td>
        <td><%= entry.title %></td>
        <td><%= entry.description %></td>
        <td><%= entry.picture %></td>
        <td><%= entry.link1 %></td>
        <td><%= entry.link2 %></td>
        <td><%= entry.guid %></td>
        <td><%= entry.status %></td>
        <td><%= link_to 'Show', feed_entry_path(@feed, entry) %></td>
        <td><%= link_to 'Edit', edit_feed_entry_path(@feed, entry) %></td>
        <td><%= link_to 'Destroy', feed_entry_path(@feed, entry), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'To Feed', feed_path(@feed) %>

<%= link_to 'New Entry', new_feed_entry_path(@feed) %>
